package pages;

import javax.swing.JPanel;
import javax.swing.JTextField;
import javax.swing.JLabel;
import javax.swing.JButton;

public class Top_Page_Edit extends JPanel {
	private JTextField textField;
	private JButton btnSelectTags;

	/**
	 * Create the panel.
	 */
	public Top_Page_Edit() {
		
		textField = new JTextField();
		textField.setColumns(10);
		add(textField);
		
		JLabel label = new JLabel("task text");
		add(label);
		
		btnSelectTags = new JButton("Select Tags");
		add(btnSelectTags);

	}

	public JButton getBtnSelectTags() {
		return btnSelectTags;
	}
	public JTextField getTextField() {
		return textField;
	}
}
